MDScreen:
    name: "menu_profile_second"
    MDFloatLayout:
        md_bg_color: rgba(246, 250, 255, 255)
        ProfileCard:
            size_hint_y: .65
            pos_hint: {"center_y": .7}
            elevation: 3
            md_bg_color: 1, 1, 1, 1
            radius: [0, 0, 20, 20]
            MDIconButton:
                icon: "arrow-left"
                pos_hint: {"center_y": .91}
                user_font_size: "20sp"
                theme_text_color: "Custom"
                text_color: rgba(34, 34, 34, 255)
                on_release:
                    root.manager.transition.direction = "right"
                    root.manager.current = "hospital_book"
            MDIconButton:
                icon: "dots-vertical"
                pos_hint: {"center_x": .93, "center_y": .91}
                user_font_size: "20sp"
                theme_text_color: "Custom"
                text_color: rgba(34, 34, 34, 255)
            MDLabel:
                text: "My Profile"
                font_name: "Roboto"
                font_size: "20sp"
                pos_hint: {"center_x": .56, "center_y": .82}
                color: rgba(34, 34, 34, 255)
            MDFloatLayout:
                orientation:'vertical'
